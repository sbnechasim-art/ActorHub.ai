{
  "name": "@actorhub/sdk",
  "version": "1.0.0",
  "description": "Official ActorHub.ai SDK for identity verification and licensing",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "require": "./dist/index.js",
      "import": "./dist/index.mjs",
      "types": "./dist/index.d.ts"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup src/index.ts --format cjs,esm --dts",
    "dev": "tsup src/index.ts --format cjs,esm --dts --watch",
    "test": "vitest",
    "lint": "eslint src/",
    "clean": "rimraf dist"
  },
  "keywords": [
    "actorhub",
    "identity",
    "verification",
    "face-recognition",
    "ai",
    "deepfake",
    "protection"
  ],
  "author": "ActorHub.ai",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/actorhub/sdk"
  },
  "dependencies": {
    "axios": "^1.6.0"
  },
  "devDependencies": {
    "@types/node": "^20.10.0",
    "tsup": "^8.0.0",
    "typescript": "^5.3.0",
    "vitest": "^1.0.0",
    "rimraf": "^5.0.0"
  },
  "peerDependencies": {
    "typescript": ">=4.7.0"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
